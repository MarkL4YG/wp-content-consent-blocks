{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wp-content-consent-blocks/plain-html-consent",
  "version": "0.1.0",
  "title": "HTML embedding consent",
  "category": "embed",
  "icon": "smiley",
  "description": "Allows user to consent to loading some plain HTML (possibly containing scripts).",
  "example": {},
  "supports": {
    "color": {
      "text": true,
      "background": true
    },
    "html": true
  },
  "attributes": {
    "disclaimerHtml": {
      "type": "string"
    },
    "contentHtml": {
      "type": "string"
    },
    "enableBtnCaption": {
      "type": "string"
    },
    "disableBtnCaption": {
      "type": "string"
    },
    "consentId": {
      "type": "string"
    }
  },
  "textdomain": "wp-content-consent-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}